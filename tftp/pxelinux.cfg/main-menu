INCLUDE pxelinux.cfg/main-menu.cfg/common

MENU TITLE Mirror PXE boot -- Main menu
DEFAULT menu.c32
PROMPT 0
ONTIMEOUT local

LABEL local
  MENU LABEL ^Boot from local hard drive
  KERNEL chain.c32

MENU SEPARATOR

LABEL vanilla
  MENU LABEL ^Vanilla OS installations
  KERNEL menu.c32
  APPEND pxelinux.cfg/autogenerated-menu-vanilla

LABEL custom
  MENU LABEL ^Custom  OS installations
  KERNEL menu.c32
  APPEND pxelinux.cfg/autogenerated-menu-custom

LABEL troubleshooting
  MENU LABEL ^Troubleshooting tools
  KERNEL menu.c32
  APPEND pxelinux.cfg/autogenerated-menu-troubleshooting

